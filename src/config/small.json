{
  "Data": {
    "name": "imagenet",
    "root_path": "/Tim/Data/ImageNet",
    "num_class": 1001,
    "input_size": 224,
    "batch_size": 16
  },
  "Augmentation": {
    "color": true,
    "affine": true,
    "flip": true
  },
  "Graph": {
    "mode": "ws",
    "ws": {
      "n": 32,
      "k": 4,
      "p": 0.75
    },
    "num_random_dag_parts": 3,
    "dag_def": []
  },
  "Model": {
    "arch": "small",
    "base_channel": 64,
    "weight_decay": 5e-5,
    "dropout": 0.5
  },
  "Train": {
    "epoch": 20,
    "optimizer": "adam",
    "lr_policy": "step_lr",
    "step_lr": {
      "time": [
        2000
      ],
      "value": [
        1e-3,
        1e-3
      ]
    },
    "cos_lr": {
      "init": 1e-3,
      "step": 500000,
      "warmup": 3000
    },
    "exp_lr": {
      "init": 1e-2,
      "decay_rate": 0.1,
      "decay_step": 10000
    },
    "label_smooth": 0.1
  },
  "Monitor": {
    "log_step": 500,
    "ckpt_save_epoch": 1,
    "max_eval_step": 1000
  }
}